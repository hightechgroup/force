{
  "AllowedHosts": "*",
  "ConnectionStrings":
  {
    "DefaultConnection": "Host=localhost;Port=6000;Database=force_db;Username=postgres;Password=postgres"
  },
  "Serilog": {
    "Using": ["Serilog.Sinks.Graylog"],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Graylog",
        "Args": {
          "hostnameOrAddress": "localhost",
          "port": "12201",
          "transportType": "Udp"
        }
      },
      {
        "Name": "Console"
      }
    ]
  }
}
